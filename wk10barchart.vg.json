{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 1000,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/zxuu0105/3179wk9Homework/refs/heads/main/datasource-au.csv",  
    "format": {"type": "csv"}
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "State",
      "type": "nominal",
      "axis": {"labelAngle": 0}, 
      "title": "State"
    },
    "y": {
      "field": "2015", 
      "type": "quantitative",
      "title": "Number of Immigrations in 2015"
    },
    "color": {
      "field": "State", 
      "type": "nominal",
      "legend": null  
    }
  },
  "config": {
    "axis": {
      "labelFontSize": 12,
      "titleFontSize": 14
    }
  }
}